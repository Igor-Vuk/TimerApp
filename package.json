{
  "name": "reacttimer",
  "version": "1.0.0",
  "description": "React Timer and Countdown app",
  "main": "index.js",
  "author": "Igor Vukelic",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/build/index.js",
    "start-dev": "set \"NODE_ENV=development\" && nodemon ./start-dev.js",
    "clean-server": "rm -rf ./src/build",
    "clean-client": "rm -rf ./src/client/dist",
    "build-server": "babel src/server -d src/build -s --copy-files",
    "build-client": "webpack --config webpack.config.babel.js",
    "webpack": "webpack -w --config webpack.config.babel.js",
    "flow": "./node_modules/.bin/flow check",
    "standard": "node_modules/.bin/standard --verbose | node_modules/.bin/snazzy"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.43.0",
    "nodemon": "^1.11.0",
    "snazzy": "^6.0.0",
    "standard": "^10.0.1",
    "webpack": "^2.3.2"
  },
  "repository": "git@github.com:Igor-Vuk/TimerApp.git"
}
